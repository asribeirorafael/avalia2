<ol class="breadcrumb">
    <li><a ng-click="alterarPagina('dashboard', '#Content')">Dashboard</a></li>
    <li><a ng-click="alterarPagina('listaTurmasHipotese', '#Content')">Turmas</a></li>
    <li><a ng-click="alterarPagina('listAvaliacoesHipotese', '#Content')">Avaliações</a></li>
    <li class="active">{{'Avaliacao Hipotese '+avaliacaoSelecionada.periodo+'ºPeriodo de '+avaliacaoSelecionada.ano}}</li>
</ol>

<div>
    <div class="col-md-8">
        <div><h1>Hipótese de Escrita</h1></div>
        <div>
            <ul class="wrap-check">
                <li>
                    <div>
                        <input id="chkbx1" ng-change="UTILIDADE.validarUnico(respostaAvaliacaoHipotese.nivelHipotese)" ng-model="respostaAvaliacaoHipotese.nivelHipotese" ng-true-value="1" ng-false-value="" class="prettycheckbox" type="checkbox" style="width: 110px; height: 110px;vertical-align: bottom;position: relative;top: 15px;overflow: hidden;"/>
                        <label><h2>Alfabético</h2></label>
                    </div>
                </li>
                <li>
                    <div>
                        <input id="chkbx2" ng-change="UTILIDADE.validarUnico(respostaAvaliacaoHipotese.nivelHipotese)" ng-model="respostaAvaliacaoHipotese.nivelHipotese" ng-true-value="2" ng-false-value="" class="prettycheckbox" type="checkbox" style="width: 110px; height: 110px;vertical-align: bottom;position: relative;top: 15px;overflow: hidden;"/>
                        <label><h2>Silábico-Alfabético</h2></label>
                    </div>
                </li>
                <li>
                    <div>
                        <input id="chkbx3" ng-change="UTILIDADE.validarUnico(respostaAvaliacaoHipotese.nivelHipotese)" ng-model="respostaAvaliacaoHipotese.nivelHipotese" ng-true-value="3" ng-false-value="" class="prettycheckbox" type="checkbox" style="width: 110px; height: 110px;vertical-align: bottom;position: relative;top: 15px;overflow: hidden;"/>
                        <label><h2>Silábico com Valor</h2></label>
                    </div>
                </li>
                <li>
                    <div>
                        <input id="chkbx4" ng-change="UTILIDADE.validarUnico(respostaAvaliacaoHipotese.nivelHipotese)" ng-model="respostaAvaliacaoHipotese.nivelHipotese" ng-true-value="4" ng-false-value="" class="prettycheckbox" type="checkbox" style="width: 110px; height: 110px;vertical-align: bottom;position: relative;top: 15px;overflow: hidden;"/>
                        <label><h2>Silábico sem Valor</h2></label>
                    </div>
                </li>
                <li>
                    <div>
                        <input id="chkbx5" ng-change="UTILIDADE.validarUnico(respostaAvaliacaoHipotese.nivelHipotese)" ng-model="respostaAvaliacaoHipotese.nivelHipotese" ng-true-value="5" ng-false-value="" class="prettycheckbox" type="checkbox" style="width: 110px; height: 110px;vertical-align: bottom;position: relative;top: 15px;overflow: hidden;"/>
                        <label><h2>Pré-Silábico</h2></label>
                    </div>
                </li>
            </ul>
        </div>
    </div>
    <div class="col-md-4">
        <div><a href="#" class="btn btn-primary btn-lg btn-block blue large">Análise da Avaliação</a></div>
        <div>
            <div style="margin: 2% 0 2% 0">
                <div style="float: left; margin:0 2% 0 0"><a ng-click="AVALIACAO.goToFirst()" class="btn default middle" style="width: 65px; height: 65px"><h2 style="margin: 8px 0 0 0px;"><<</h2></a></div>
                <div style="float: left; margin:0 2% 0 0"><a ng-click="AVALIACAO.goToPrior()" class="btn default middle" style="width: 65px; height: 65px"><h2 style="margin: 8px 0 0 0px;"><</h2></a></div>
                <div style="border: 1px solid #cccccc; float: left; height: 65px; width: 71px; margin:0 2% 0 0"><h2 style="margin: 20px 0 0 8px;">{{(indiceAluno+1)+"/"+turmaSelecionada.Alunos.length}}</h2></div>
                <div style="float: left; margin:0 2% 0 0"><a ng-click="AVALIACAO.goToNext()" class="btn default middle" style="width: 65px; height: 65px"><h2 style="margin: 8px 0 0 0px;">></h2></a></div>
                <div><a ng-click="AVALIACAO.goToLast()" class="btn default middle" style="width: 65px; height: 65px"><h2 style="margin: 8px 0 0 0px;">>></h2></a></div>
            </div>
            <div style="border: 1px solid #cccccc">
                <div style="padding: 5% 28%;">
                    <img src="{{alunoSelecionadoHipotese.imagem}}" style="height: 150px; width: 150px">
                </div>
                <div>
                    <div style="padding: 5% 0 3% 5%;"><b>Nome:</b>{{alunoSelecionadoHipotese.nome}}</div>
                    <div style="padding: 0 0 5% 5%;"><b>Data:</b>{{alunoSelecionadoHipotese.dataNascimento}}</div>
                </div>
            </div>
        </div>
        <div style="border: 1px solid #cccccc; margin-top: 3%">
            <div style="padding: 0 0 0% 28%;"><b>Observação do Aluno:</b></div>
            <div style="border: 1px solid #cccccc; margin: 5%; height: 217px"></div>
        </div>
    </div>
</div>